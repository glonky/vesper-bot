{
  "name": "@vesper/discord-bot",
  "version": "1.0.0",
  "author": "Glonky <glonky.sailor@gmail.com>",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/glonky/vesper-discord-bot.git"
  },
  "engines": {
    "node": ">=16"
  },
  "license": "MIT",
  "scripts": {
    "prepare": "is-ci || husky install",
    "start": "ts-node-dev --respawn --transpile-only src/server.ts & ts-node-dev --respawn --transpile-only src/scripts/deploy-commands.ts",
    "heroku": "ts-node src/server.ts",
    "deploy:commands": "ts-node src/scripts/deploy-commands.ts",
    "test": "jest",
    "build": "tsc",
    "heroku-postbuild": "yarn build && node dist/scripts/deploy-commands.js",
    "lint": "eslint --ext .json,.ts,.tsx --cache .",
    "lint:fix": "yarn lint --fix",
    "prepublishOnly": "yarn build"
  },
  "dependencies": {
    "@discordjs/builders": "0.8.2",
    "@discordjs/rest": "0.1.0-canary.0",
    "@type-cacheable/core": "10.0.2",
    "@type-cacheable/ioredis-adapter": "10.0.2",
    "async-retry": "1.3.3",
    "bignumber.js": "9.0.1",
    "coingecko-api": "1.0.10",
    "dedent-js": "1.0.1",
    "discord-api-types": "0.24.0",
    "discord.js": "13.3.1",
    "dotenv-expand": "5.1.0",
    "dotenv-flow": "3.2.0",
    "easy-table": "1.2.0",
    "glob": "7.2.0",
    "http-status-codes": "2.1.4",
    "ioredis": "4.28.0",
    "lodash": "4.17.21",
    "luxon": "2.1.1",
    "moment-timezone": "0.5.34",
    "node-fetch": "2.6.6",
    "pino": "7.0.5",
    "pino-pretty": "7.1.0",
    "pluralize": "8.0.0",
    "reflect-metadata": "0.1.13",
    "typedi": "0.10.0",
    "uuid": "8.3.2",
    "web3": "1.6.0"
  },
  "devDependencies": {
    "@commitlint/cli": "14.1.0",
    "@commitlint/config-conventional": "14.1.0",
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/commit-analyzer": "9.0.1",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "8.0.2",
    "@semantic-release/npm": "8.0.3",
    "@semantic-release/release-notes-generator": "10.0.2",
    "@types/async-retry": "1.4.3",
    "@types/bignumber.js": "5.0.0",
    "@types/dotenv-flow": "3.2.0",
    "@types/easy-table": "1.2.0",
    "@types/glob": "7.2.0",
    "@types/http-status-codes": "1.2.0",
    "@types/ioredis": "4.28.1",
    "@types/jest": "27.0.2",
    "@types/lodash": "4.14.176",
    "@types/luxon": "2.0.7",
    "@types/moment-timezone": "0.5.30",
    "@types/node": "16.11.7",
    "@types/node-notifier": "8.0.1",
    "@types/pino": "6.3.12",
    "@types/pluralize": "0.0.29",
    "@types/uuid": "8.3.1",
    "@typescript-eslint/eslint-plugin": "5.3.1",
    "@typescript-eslint/parser": "5.3.1",
    "commitizen": "4.2.4",
    "concurrently": "6.4.0",
    "conventional-changelog-cli": "2.1.1",
    "cz-conventional-changelog": "3.3.0",
    "env-cmd": "10.1.0",
    "eslint": "8.2.0",
    "eslint-config-airbnb-typescript": "15.0.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jest": "25.2.4",
    "eslint-plugin-jest-formatting": "3.0.0",
    "eslint-plugin-json": "3.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-sort-keys-fix": "1.1.2",
    "eslint-plugin-unused-imports": "2.0.0",
    "husky": "7.0.4",
    "is-ci": "3.0.1",
    "jest": "27.3.1",
    "lint-staged": "12.0.1",
    "prettier": "2.4.1",
    "semantic-release": "18.0.0",
    "ts-jest": "27.0.7",
    "ts-node": "10.4.0",
    "ts-node-dev": "1.1.8",
    "tslib": "2.3.1",
    "typescript": "4.4.4"
  },
  "lint-staged": {
    "**/*.{ts,tsx,js}": [
      "eslint --fix --cache",
      "yarn test --ci --silent --forceExit --detectOpenHandles --bail --findRelatedTests --coverage=false"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
