{
  "name": "@vesper/discord-bot",
  "version": "1.0.0",
  "author": "Glonky <glonky.sailor@gmail.com>",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/glonky/vesper-discord-bot.git"
  },
  "engines": {
    "node": ">=16"
  },
  "license": "MIT",
  "scripts": {
    "prepare": "is-ci || husky install",
    "start": "ts-node-dev --respawn --transpile-only src/server.ts",
    "heroku": "ts-node src/server.ts",
    "deploy:commands": "ts-node src/scripts/deploy-commands.ts",
    "test": "jest",
    "build": "tsc",
    "heroku-postbuild": "yarn build && && yarn deploy:commands",
    "lint": "eslint --ext .json,.ts,.tsx --cache .",
    "lint:fix": "yarn lint --fix",
    "prepublishOnly": "yarn build"
  },
  "dependencies": {
    "@discordjs/builders": "0.7.0",
    "@discordjs/rest": "0.1.0-canary.0",
    "discord-api-types": "0.24.0",
    "discord.js": "13.2.0",
    "dotenv-expand": "5.1.0",
    "dotenv-flow": "3.2.0",
    "glob": "^7.2.0",
    "lodash": "4.17.21",
    "moment-timezone": "0.5.33",
    "pino": "7.0.3",
    "pino-pretty": "7.1.0",
    "typedi": "0.8.0",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "13.2.1",
    "@commitlint/config-conventional": "13.2.0",
    "@semantic-release/changelog": "6.0.0",
    "@semantic-release/commit-analyzer": "9.0.1",
    "@semantic-release/git": "10.0.0",
    "@semantic-release/github": "8.0.1",
    "@semantic-release/npm": "8.0.1",
    "@semantic-release/release-notes-generator": "10.0.2",
    "@types/dotenv-flow": "3.1.1",
    "@types/glob": "^7.2.0",
    "@types/jest": "27.0.2",
    "@types/lodash": "4.14.175",
    "@types/moment-timezone": "0.5.30",
    "@types/node": "16.11.1",
    "@types/node-notifier": "8.0.1",
    "@types/pino": "6.3.12",
    "@types/uuid": "8.3.1",
    "@typescript-eslint/eslint-plugin": "5.1.0",
    "@typescript-eslint/parser": "5.1.0",
    "commitizen": "4.2.4",
    "concurrently": "6.3.0",
    "conventional-changelog-cli": "2.1.1",
    "cz-conventional-changelog": "3.3.0",
    "env-cmd": "10.1.0",
    "eslint": "8.0.1",
    "eslint-config-airbnb-typescript": "14.0.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-jest": "25.2.2",
    "eslint-plugin-jest-formatting": "3.0.0",
    "eslint-plugin-json": "3.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-sort-keys-fix": "1.1.2",
    "eslint-plugin-unused-imports": "1.1.5",
    "husky": "7.0.2",
    "is-ci": "3.0.0",
    "jest": "27.3.1",
    "lint-staged": "11.2.3",
    "prettier": "2.4.1",
    "reflect-metadata": "0.1.13",
    "semantic-release": "18.0.0",
    "ts-jest": "27.0.7",
    "ts-node": "10.3.0",
    "ts-node-dev": "1.1.8",
    "tslib": "2.3.1",
    "typescript": "4.4.4"
  },
  "lint-staged": {
    "**/*.{ts,tsx,js}": [
      "eslint --fix --cache",
      "yarn test --ci --silent --forceExit --detectOpenHandles --bail --findRelatedTests --coverage=false"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
