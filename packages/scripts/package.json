{
  "name": "@vesper-discord/scripts",
  "version": "1.0.0",
  "type": "commonjs",
  "exports": "./dist/index.js",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "version": "oclif-dev readme && git add README.md"
  },
  "bin": {
    "vesper": "./bin/run"
  },
  "dependencies": {
    "@discordjs/rest": "0.3.0",
    "@oclif/command": "1.8.16",
    "ethers": "5.6.1",
    "@oclif/config": "1.18.3",
    "@oclif/plugin-autocomplete": "1.2.0",
    "@oclif/plugin-commands": "2.1.0",
    "@oclif/plugin-help": "5.1.12",
    "@oclif/plugin-not-found": "2.3.1",
    "@vesper-discord/command-bot": "1.0.0",
    "@vesper-discord/config": "1.0.0",
    "@vesper-discord/redis-service": "1.0.0",
    "@vesper-discord/discord-service": "1.0.0",
    "@vesper-discord/etherscan-service": "1.0.0",
    "@vesper-discord/errors": "1.0.0",
    "@vesper-discord/logger": "1.0.0",
    "async-retry": "1.3.3",
    "cli-ux": "6.0.9",
    "discord-api-types": "0.29.0",
    "glob": "7.2.0",
    "inquirer": "8.2.1",
    "listr": "0.14.3",
    "lodash": "4.17.21",
    "reflect-metadata": "0.1.13",
    "typedi": "0.10.0",
    "ulid": "2.3.0"
  },
  "devDependencies": {
    "@types/async-retry": "1.4.3",
    "@types/glob": "7.2.0",
    "@types/inquirer": "8.2.0",
    "@types/lodash": "4.14.180",
    "@types/uuid": "8.3.4"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "oclif": {
    "commands": "./dist/commands",
    "bin": "vesper",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-commands",
      "@oclif/plugin-not-found"
    ]
  }
}
