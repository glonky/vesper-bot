{
  "name": "@vesper-discord/command-bot",
  "version": "1.0.0",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "start": "yarn start:server & yarn deploy:commands:watch",
    "start:server": "ts-node-dev --respawn --transpile-only src/index.ts",
    "heroku": "ts-node src/index.ts",
    "deploy:commands": "ts-node src/scripts/deploy-commands.ts",
    "deploy:commands:watch": "ts-node-dev --respawn --transpile-only src/scripts/deploy-commands.ts",
    "heroku-postbuild": "yarn build && node dist/scripts/deploy-commands.js"
  },
  "dependencies": {
    "@discordjs/builders": "0.11.0",
    "@discordjs/rest": "0.2.0-canary.0",
    "@type-cacheable/core": "10.0.3",
    "@type-cacheable/ioredis-adapter": "10.0.3",
    "async-retry": "1.3.3",
    "bignumber.js": "9.0.2",
    "coingecko-api": "1.0.10",
    "dedent-js": "1.0.1",
    "discord-api-types": "0.26.1",
    "discord.js": "13.5.1",
    "dotenv-expand": "5.1.0",
    "dotenv-flow": "3.2.0",
    "easy-table": "1.2.0",
    "glob": "7.2.0",
    "http-status-codes": "2.2.0",
    "ioredis": "4.28.3",
    "lodash": "4.17.21",
    "luxon": "2.3.0",
    "moment-timezone": "0.5.34",
    "node-fetch": "2.6.6",
    "pino": "7.6.3",
    "pino-pretty": "7.3.0",
    "pluralize": "8.0.0",
    "reflect-metadata": "0.1.13",
    "typedi": "0.10.0",
