{
  "name": "@vesper-discord/command-bot",
  "version": "1.0.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "start": "yarn docker:start && yarn start:server && yarn vesper deploy-commands",
    "start:server": "tsc -b -w & ts-node-dev --respawn --transpile-only src/index.ts",
    "docker:start": "docker compose up -d",
    "heroku": "ts-node src/index.ts",
    "deploy-commands": "yarn vesper deploy-discord-commands -c ./src/commands",
    "heroku-postbuild": "yarn build && CI=true yarn deploy-commands"
  },
  "dependencies": {
    "@vesper-discord/config": "1.0.0",
    "@vesper-discord/errors": "1.0.0",
    "@vesper-discord/logger": "1.0.0",
    "@vesper-discord/retry": "1.0.0",
    "@vesper-discord/utils": "1.0.0",
    "@vesper-discord/discord-service": "1.0.0",
    "@vesper-discord/coin-gecko-service": "1.0.0",
    "@vesper-discord/etherscan-service": "1.0.0",
    "@vesper-discord/vesper-service": "1.0.0",
    "@vesper-discord/redis-service": "1.0.0",
    "bignumber.js": "9.0.2",
    "lodash": "4.17.21",
    "typedi": "0.10.0",
    "web3": "1.6.1",
    "reflect-metadata": "0.1.13"
  },
  "devDependencies": {
    "@types/bignumber.js": "5.0.0",
    "@types/lodash": "4.14.178"
  }
}
